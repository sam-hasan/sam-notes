    "url": "https://github.com/sam-hasan/sam-notes/issues"
  },
  "homepage": "https://github.com/sam-hasan/sam-notes#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "html-to-text": "^8.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.5",
    "morgan": "^1.10.0",
    "slugify": "^1.6.0",
    "validator": "^13.6.0"
  },
